<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="stackPane" prefHeight="207.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafx.controller.SignInController">
    <children>
        <StackPane>
            <children>
                <VBox spacing="10.0">
                    <children>
                        <GridPane prefHeight="95.0" prefWidth="328.0">
                            <children>
                                <Label text="Email:" GridPane.columnIndex="0" GridPane.rowIndex="0">
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="Password:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                                <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="0">
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </TextField>
                                <PasswordField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0"
                                                   prefWidth="109.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0"
                                                   prefWidth="189.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="61.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="46.599999999999994" minHeight="4.599999999999994"
                                                prefHeight="40.599999999999994" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                            <VBox.margin>
                                <Insets top="20.0"/>
                            </VBox.margin>
                        </GridPane>
                        <StackPane prefHeight="-1.0" prefWidth="-1.0">
                            <children>
                                <Button fx:id="loginButton" alignment="CENTER" defaultButton="true"
                                        mnemonicParsing="false" onAction="#postLogin" text="Login"
                                        StackPane.alignment="CENTER_RIGHT"/>
                            </children>
                        </StackPane>
                        <Label fx:id="lblError" alignment="CENTER" prefHeight="18.0" prefWidth="339.0"/>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</StackPane>
